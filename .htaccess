# Enable Rewrite Engine
RewriteEngine On

# Hapus .php dari URL
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [L]

# Redirect /about.php ke /about (opsional, agar SEO friendly)
RewriteCond %{THE_REQUEST} \s([^.]+)\.php
RewriteRule ^ %1 [R=301,L]
